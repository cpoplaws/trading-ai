# Alpaca API Keys (Paper Trading)
ALPACA_API_KEY=your_alpaca_api_key_here
ALPACA_SECRET_KEY=your_alpaca_secret_key_here

# Binance API Keys (optional CEX connector)
BINANCE_API_KEY=your_binance_api_key_here
BINANCE_API_SECRET=your_binance_secret_here

# Wallet encryption password (required for real wallet operations)
WALLET_MASTER_PASSWORD=replace_with_secure_password

# Optional infrastructure dependencies
DATABASE_URL=postgresql://user:password@host:5432/trading_ai
REDIS_URL=redis://host:6379/0

# CORS allowlist (comma separated)
CORS_ALLOW_ORIGINS=http://localhost:3000,https://your-frontend.vercel.app

# Port (Railway will set this automatically)
PORT=8000
