# Trading AI Environment Variables
# Copy this file to .env and fill in your API keys

# API Keys
ALPHA_VANTAGE_API_KEY=your_alpha_vantage_key_here
ALPACA_API_KEY=your_alpaca_key_here
ALPACA_SECRET_KEY=your_alpaca_secret_here
IB_API_KEY=your_interactive_brokers_key_here

# DeFi and Blockchain
BSC_PRIVATE_KEY=your_bsc_wallet_private_key_here
ETH_PRIVATE_KEY=your_ethereum_wallet_private_key_here
BSC_RPC_URL=https://bsc-dataseed1.binance.org/
BSC_TESTNET_RPC_URL=https://data-seed-prebsc-1-s1.binance.org:8545/

# Database (if using full profile)
DATABASE_URL=postgresql://trader:secure_password@postgres:5432/trading_ai
REDIS_URL=redis://redis:6379/0

# Environment
ENVIRONMENT=development
DEBUG=true

# Logging
LOG_LEVEL=INFO

# Trading settings
PAPER_TRADING=true
MAX_POSITION_SIZE=0.1
RISK_TOLERANCE=moderate
